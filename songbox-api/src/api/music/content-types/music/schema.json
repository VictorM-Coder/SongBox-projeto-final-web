{
  "kind": "collectionType",
  "collectionName": "musics",
  "info": {
    "singularName": "music",
    "pluralName": "musics",
    "displayName": "Music",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "artist": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::artist.artist"
    },
    "MinuteDuration": {
      "type": "integer",
      "min": 0,
      "required": true
    },
    "DurationSeconds": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 59
    },
    "release": {
      "type": "date",
      "required": true
    },
    "lirics": {
      "type": "richtext",
      "required": true,
      "minLength": 1
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": false
    },
    "playlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::playlist.playlist",
      "mappedBy": "music"
    }
  }
}
